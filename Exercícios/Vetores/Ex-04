import java.util.Scanner;

public class exe4 {
    public static void main(String[] args){
        Scanner Ler = new Scanner(System.in);
        System.out.println("Limite: ");
        int n = Ler.nextInt();
        int vetor[] = new int[n];
        for (int i = 0; i<n; i++){
            System.out.println("Num: ");
            vetor[i] = Ler.nextInt();
        }
        for (int i = n-1; i>=0; i--){
            System.out.println(vetor[i]);
        }
    }
}
